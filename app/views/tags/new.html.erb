<%= form_for :tag ,url: "/resources/", :html => { :multipart => true } do |f| %>

<div class="control-group">
      <%= f.label :tags, "Tags (separated by commas)", :class => 'control-label'  %>
      <div class="controls">
        <%= f.text_field :tag_list, :class => 'text_field' %>
      </div>
    </div>


<div class="form-actions">
      <%= f.submit nil, :class => 'btn btn-primary'  %>
      <%= link_to 'Cancel', resources_path, :class => 'btn' %>
    </div>

<% end %>
